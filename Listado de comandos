


#instalar paquetes
 sudo apt-get install git

#Kubernetes
wget https://storage.googleapis.com/kubernetes-release/release/v1.10.1/bin/linux/amd64/kubeadm    
wget https://storage.googleapis.com/kubernetes-release/release/v1.10.1/bin/linux/amd64/kubectl    
wget https://storage.googleapis.com/kubernetes-release/release/v1.10.1/bin/linux/amd64/kubelet


Sudo kubectl get pods --all-namespaces #Ver todos los pods 
sudo kubectl ngix-app -delete # eliminar nodos



# Dockers
sudo docker ps -a # ver listado de dockers




# descargar docker y ejecutarlo
docker pull jenkins/jenkins
sudo docker run -p 8080:8080 -p 50000:50000 -v jenkins_home:/var/jenkins_home jenkins/jenkins:lts-jdk1

# git adicionar directorio
git config --global user.name"jprios"  #crear usuarios
git init  # arrancar git
git add <folder/Archivo>
git add -A #Adiciona todos lo que este en la ruta
git add '/home/jprios/Documentos/Proyectos JP/Prueba Git-github'
git status
git commit -m "mensaje del commit" 
git pull <branch name> <remote url>
git push origin <branch name>  
git rm <folder/Archivo>

